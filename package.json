{
  "name": "mixed-nuts",
  "version": "1.0.1",
  "description": "ミックスナッツ〜ウェブデザイナ向け開発環境〜",
  "main": "index.js",
  "private": true,
  "scripts": {
    "git:init": "rimraf ./.git; git init; git add .; git commit -m 'first commit'",
    "clean:all": "rimraf ./public; rimraf ./package-lock.json; rimraf ./node_modules",
    "clean": "rimraf ./public",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "npm run clean; cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "netlify": "netlify",
    "dep": "npm run production; netlify deploy",
    "deploy": "netlify deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ucan-lab@github.com/ucan-lab/mixed-nuts.git"
  },
  "keywords": [],
  "author": "ucan-lab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ucan-lab/mixed-nuts/issues"
  },
  "homepage": "https://github.com/ucan-lab/mixed-nuts#readme",
  "devDependencies": {
    "axios": "^0.17",
    "bootstrap-sass": "^3.3.7",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^5.1",
    "ejs-compiled-loader": "^1.1.0",
    "extract-loader": "^1.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.4",
    "jquery": "^3.3.1",
    "laravel-mix": "^2.0",
    "laravel-mix-purgecss": "^1.0.5",
    "lodash": "^4.17.4",
    "netlify-cli": "^1.2.2",
    "rimraf": "^2.6.2",
    "vue": "^2.5.7"
  }
}
